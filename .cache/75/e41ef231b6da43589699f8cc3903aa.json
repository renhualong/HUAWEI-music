\r\n\r\n          &.current {\r\n            color: $color;\r\n          }\r\n        }\r\n      }\r\n\r\n    }\r\n\r\n\r\n\r\n  }\r\n\r\n  .footer {\r\n    height: 160px;\r\n\r\n\r\n    .buttons {\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n\r\n\r\n      svg {\r\n        height: 24px;\r\n        width: 24px;\r\n        margin: 0 20px;\r\n      }\r\n    }\r\n\r\n    .area-bar {\r\n      color: $color1;\r\n      font-size: 12px;\r\n      display: flex;\r\n      padding: 0 20px;\r\n      margin-top: 20px;\r\n      align-items: center;\r\n\r\n      .time-start {\r\n        order: 1;\r\n      }\r\n\r\n      .time-end {\r\n        order: 3;\r\n      }\r\n\r\n      .bar {\r\n        order: 2;\r\n        flex: 1;\r\n        height: 4px;\r\n        background: $color3;\r\n        border-radius: 2px;\r\n        margin: 0 10px;\r\n      }\r\n\r\n      .progress {\r\n        width: 0%;\r\n        height: 100%;\r\n        border-radius: 2px;\r\n        background: $color2;\r\n        position: relative;\r\n\r\n        &::after {\r\n          content: '';\r\n          position: absolute;\r\n          right: 0;\r\n          top: -7px;\r\n          display: block;\r\n          width: 16px;\r\n          height: 16px;\r\n          background: url('../svg/progress.svg') 0 0 no-repeat;\r\n          background-size: contain;\r\n\r\n        }\r\n      }\r\n\r\n    }\r\n\r\n    .actions {\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: space-between;\r\n      margin-top: 10px;\r\n      padding: 0 20px;\r\n\r\n      svg {\r\n        width: 28px;\r\n        height: 28px;\r\n\r\n        &.btn-play-pause {\r\n          width: 50px;\r\n          height: 50px;\r\n        }\r\n      }\r\n\r\n    }\r\n  }\r\n\r\n}\r\n\r\n@keyframes rotate {\r\n  0% {\r\n    transform: rotate(0);\r\n  }\r\n\r\n  100% {\r\n    transform: rotate(360deg);\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"fc52ff3051181596d2931403badb56